{
  "name": "bubblifier-bot",
  "version": "0.0.1",
  "description": "telegram bot for bubble sticker packs creation",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "lint": "eslint \"**/*.ts\"",
    "start": "node build/index.js",
    "prod": "npm run build && npm run start"
  },
  "keywords": [
    "telegram",
    "stickers",
    "bot"
  ],
  "author": "pool-party",
  "license": "MIT",
  "repository": "github:pool-party/bubblifier-bot",
  "dependencies": {
    "@types/node": "^14.14.22",
    "@types/sharp": "^0.27.1",
    "got": "^11.8.1",
    "prisma": "^2.16.0",
    "sharp": "^0.27.1",
    "telegraf": "^4.0.2"
  },
  "devDependencies": {
    "@prisma/client": "2.16.0",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
